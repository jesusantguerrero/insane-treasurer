{
    "name": "insane/treasurer",
    "description": "Like cashier but for paypal",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "freesgen",
            "email": "jesusant.guerrero@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "dompdf/dompdf": "^0.8.6|^1.0.1",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.0",
        "paypal/rest-api-sdk-php": "^1.14"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "suggest": {
        "ext-intl": "Allows for more locales besides the default \"en\" when formatting money values."
    },
    "autoload": {
        "psr-4": {
            "Insane\\Treasurer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Insane\\Treasurer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Insane\\Paypal\\PaypalServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
