{
    "name": "insane/treasurer",
    "description": "Like cashier but for paypal",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "freesgen",
            "email": "jesusant.guerrero@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.18|^9.0",
        "dompdf/dompdf": "^2.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.4"
    },
    "suggest": {
        "ext-intl": "Allows for more locales besides the default \"en\" when formatting money values."
    },
    "autoload": {
        "psr-4": {
            "Insane\\Treasurer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Insane\\Treasurer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Insane\\Treasurer\\TreasurerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
